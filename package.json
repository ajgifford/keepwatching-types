{
  "name": "@ajgifford/keepwatching-types",
  "version": "0.6.0",
  "description": "Shared TypeScript types for KeepWatching application",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "yarn build",
    "clean": "rimraf dist",
    "rebuild": "yarn clean && yarn build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --config ./.prettierrc.js . --write",
    "type-check": "tsc --noEmit",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajgifford/keepwatching-types.git"
  },
  "author": "Andy Gifford <ajgifford@users.noreply.github.com>",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "9.37.0",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "eslint": "9.37.0",
    "eslint-config-prettier": "10.1.8",
    "prettier": "3.6.2",
    "rimraf": "^6.0.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.1"
  },
  "packageManager": "yarn@1.22.22"
}
